#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/nugget@2.2.0/node_modules/nugget/node_modules:/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/nugget@2.2.0/node_modules:/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/nugget@2.2.0/node_modules/nugget/node_modules:/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/nugget@2.2.0/node_modules:/home/becquer/Documents/c++/stop-music/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nugget/bin.js" "$@"
else
  exec node  "$basedir/../nugget/bin.js" "$@"
fi
